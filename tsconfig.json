{
   "compilerOptions": {
      "jsx": "react-jsx",
      "baseUrl": ".",
      "noEmit": true,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "allowImportingTsExtensions": true,
      "paths": {
         "@/*": ["./src/*"],
         "@components/*": ["./src/components/*"],
         "@lib/*": ["./src/lib/*"]
      }
   }
}
